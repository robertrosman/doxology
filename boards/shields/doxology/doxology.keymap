#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&kp TAB        &kp Q    &kp W             &lt 5 E       &kp R      &kp T                                                   &kp Y          &kp U                 &kp I      &kp O       &kp P      &kp A
&kp BACKSPACE  &lt 4 A  &kp S             &kp D         &kp F      &kp G                                                   &kp H          &kp J                 &kp K      &kp L       &kp O      &kp A
&kp LCTRL      &kp Z    &kp X             &kp C         &kp V      &kp B                                                   &kp N          &kp M                 &kp COMMA  &kp PERIOD  &kp MINUS  &kp O
                        &kp LEFT_COMMAND  &kp LEFT_ALT
                                                        &kp ESC  &sk LEFT_SHIFT  &kp LCTRL    &kp BACKSPACE  &kp SPACE      &kp Y
                                                        &kp V      &kp LCMD                                                &kp RIGHT_ALT  &kp LCMD
            >;
        };

    };
};
